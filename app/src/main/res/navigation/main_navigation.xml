<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:startDestination="@id/launchFragment">

    <fragment
        android:id="@+id/launchFragment"
        android:name="com.tiamosu.fly.demo.ui.fragments.LaunchFragment" />

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.tiamosu.fly.demo.ui.fragments.MainFragment"
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/sharedFragment"
        android:name="com.tiamosu.fly.demo.ui.fragments.SharedFragment"
        tools:layout="@layout/fragment_shared" />

    <fragment
        android:id="@+id/httpFragment"
        android:name="com.tiamosu.fly.demo.ui.fragments.HttpFragment"
        tools:layout="@layout/fragment_http" />

    <fragment
        android:id="@+id/basicRequestFragment"
        android:name="com.tiamosu.fly.demo.ui.fragments.BasicRequestFragment"
        tools:layout="@layout/fragment_basic_request" />

    <fragment
        android:id="@+id/uploadFragment"
        android:name="com.tiamosu.fly.demo.ui.fragments.UploadFragment"
        tools:layout="@layout/fragment_upload" />

    <fragment
        android:id="@+id/downloadFragment"
        android:name="com.tiamosu.fly.demo.ui.fragments.DownloadFragment"
        tools:layout="@layout/fragment_download" />

    <fragment
        android:id="@+id/cacheFragment"
        android:name="com.tiamosu.fly.demo.ui.fragments.CacheFragment"
        tools:layout="@layout/fragment_cache" />

    <fragment
        android:id="@+id/glideFragment"
        android:name="com.tiamosu.fly.demo.ui.fragments.GlideFragment"
        tools:layout="@layout/fragment_glide" />

    <fragment
        android:id="@+id/loadSirFragment"
        android:name="com.tiamosu.fly.demo.ui.fragments.LoadSirFragment"
        tools:layout="@layout/fragment_loadsir" />
</navigation>